<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.






    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.






        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <style name="wrap_wrap">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="match_match">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="wrap_match">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="match_wrap">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <!-- page <<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<< -->

    <style name="activity_page">
        <item name="android:splitMotionEvents">false</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:background">@color/activity_bg</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="bottom_window_page">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
        <item name="android:gravity">bottom</item>
    </style>

    <!-- page >>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>> -->


    <!-- blank <<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<< -->

    <style name="blank">
        <item name="android:layout_width">match_parent</item>
    </style>

    <style name="blank_item" parent="blank">
        <item name="android:layout_height">15dp</item>
    </style>

    <style name="blank_bottom" parent="blank">
        <item name="android:layout_height">30dp</item>
    </style>

    <!-- blank >>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>> -->


    <!-- text <<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<< -->

    <style name="text" parent="wrap_wrap">
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/gray_4</item>
    </style>

    <style name="text_huge" parent="text">
        <item name="android:textSize">@dimen/text_size_huge</item>
    </style>

    <style name="text_huge_white" parent="text_huge">
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="text_huge_black" parent="text_huge">
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="text_large" parent="text">
        <item name="android:textSize">@dimen/text_size_large</item>
    </style>

    <style name="text_large_white" parent="text_large">
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="text_large_black" parent="text_large">
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="text_big" parent="text">
        <item name="android:textSize">@dimen/text_size_big</item>
    </style>

    <style name="text_big_white" parent="text_big">
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="text_big_black" parent="text_big">
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="text_middle" parent="text">
        <item name="android:textSize">@dimen/text_size_middle</item>
    </style>

    <style name="text_middle_white" parent="text_middle">
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="text_middle_black" parent="text_middle">
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="text_middle_blue" parent="text_middle">
        <item name="android:textColor">@color/blue</item>
    </style>

    <style name="text_title" parent="text">
        <item name="android:textSize">22sp</item>
    </style>

    <style name="text_small" parent="text">
        <item name="android:textSize">@dimen/text_size_small</item>
    </style>

    <style name="text_small_white" parent="text_small">
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="text_small_black" parent="text_small">
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="text_micro" parent="text">
        <item name="android:textSize">@dimen/text_size_micro</item>
    </style>

    <style name="text_micro_white" parent="text_micro">
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="text_micro_black" parent="text_micro">
        <item name="android:textColor">@color/black</item>
    </style>
    <!-- text >>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>> -->


    <!-- Button <<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<< -->

    <style name="btn_large" parent="text_large">
        <item name="android:layout_height">64dp</item>
        <item name="android:background">@drawable/btn_blue_selector</item>
    </style>

    <style name="btn_big" parent="text_big">

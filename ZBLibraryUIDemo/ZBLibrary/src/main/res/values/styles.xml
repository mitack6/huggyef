    </style>

    <style name="content_item_remind_tv_black" parent="content_item_remind_tv">
        <item name="android:textColor">@color/gray_4</item>
    </style>

    <style name="content_item">
        <item name="android:layout_height">@dimen/common_item_height</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:gravity">center_vertical</item>

        <item name="android:orientation">horizontal</item>
    </style>

    <style name="ll_horizontal_wrap_match" parent="wrap_match">
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="ll_horizontal_match_wrap" parent="match_wrap">
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
    </style>
    <!-- linearLayout horizontal >>>>>>>>>>>>>>>>>>>>>>>>>>>> -->


    <!-- linearLayout vertical  <<<<<<<<<<<<<<<<<<<<<<<<<<<< -->
    <style name="ll_vertical_wrap_wrap" parent="wrap_wrap">
        <item name="android:gravity">center_horizontal</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="ll_vertical_match_match" parent="match_match">
        <item name="android:gravity">center_horizontal</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="ll_vertical_wrap_match" parent="wrap_match">
        <item name="android:gravity">center_horizontal</item>
        <item name="android:orientation">vertical</item>
    </style>

